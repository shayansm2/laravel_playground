{
    "name": "queracollege/laravel-health",
    "description": "Smart Laravel Health Checker Package",
    "keywords": [
        "laravel-health",
        "health",
        "health-checker",
        "laravel-health-checker"
    ],
    "homepage": "https://github.com/queracollege/laravel-health",
    "license": "MIT",
    "type": "library",
    "require": {
        "php" : "^7.1.3|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0.0"
    },
    "autoload": {
        "psr-4": {
            "QueraCollege\\LaravelHealth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QueraCollege\\LaravelHealth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QueraCollege\\LaravelHealth\\HealthServiceProvider"
            ]
        }
    }
}
